<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgmewarnai"
    tools:context=".Mewarnai1">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardNext"
        android:layout_width="45dp"
        android:layout_height="51dp"
        android:layout_margin="5dp"
        android:layout_marginStart="26dp"
        android:elevation="10dp"
        app:cardCornerRadius="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.611"
        app:layout_constraintStart_toEndOf="@+id/mewarnai1"
        app:layout_constraintTop_toTopOf="parent" >
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/nextNav"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/bluetvborder"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/baseline_arrow_right_24"/>
    </androidx.cardview.widget.CardView>

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/mewarnai1"
        android:layout_width="269dp"
        android:layout_height="269dp"
        android:layout_marginStart="68dp"
        android:background="@color/white"
        android:padding="5dp"
        android:scaleType="centerCrop"
        android:src="@drawable/mewarnai2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/circular"
        app:strokeColor="@color/bluetvborder"
        app:strokeWidth="7dp"
        tools:ignore="MissingClass,MissingConstraints" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardBack"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_margin="5dp"
        android:elevation="10dp"
        app:cardCornerRadius="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintVertical_bias="0.048">
        <ImageView
            android:id="@+id/backView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="8dp"
            android:background="@color/redarrowback"
            app:srcCompat="@drawable/baseline_reply_24"
            android:scaleType="centerCrop"/>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardHome"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_margin="5dp"
        android:elevation="10dp"
        app:cardCornerRadius="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.965"
        app:layout_constraintStart_toEndOf="@+id/cardBack"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.048">
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/home"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/kuning"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/baseline_home_24" />
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>